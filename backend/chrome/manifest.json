{
    "manifest_version": 3,
    "name": "링크 저장",
    "version": "1.0",
    "description": "링크를 저장합니다.",
    "permissions": ["activeTab","http://localhost:8000/"],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon16.png",
        "48": "images/icon48.png",
        "128": "images/icon128.png"
      }
    }
  }
  